    "start": "set NODE_ENV=production&& nodemon ./dist/server.js --port 8080 ",
    "ServerTStoJS5": "./node_modules/.bin/tsc",
    "ServerTStoJS5Watch": "./node_modules/.bin/tsc -w",
    "ServerES6toJS5": "./node_modules/.bin/babel server.es6.js -o ./fromES6/server.js",
