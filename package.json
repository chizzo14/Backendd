    "nodecluster": "nodemon ./dist/server.js CLUSTER",
    "pm2fork": "pm2 start ./dist/server.js --name=fork --watch -- 8081",
    "pm2cluster": "pm2 start ./dist/server.js --name=cluster --watch -i max -- 8082",
    "runJS5fromES6": "nodemon ./fromES6/server.js",
    "test": "./node_modules/.bin/mocha ./dist/test/productos.test.js"
  },
